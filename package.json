{
  "name": "jsonschema-to-markdown",
  "version": "1.0.0",
  "description": "Convert JSON to markdown.",
  "main": "dist/index.js",
  "bin": {
    "j2m": "dist/index.js"
  },
  "config": {
    "logoutput": "dist/log.txt"
  },
  "private": true,
  "scripts": {
    "test": "ts-mocha 'test/**/*.ts'",
    "build": "tsc -p .",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "start": "node dist/index.js",
    "prestart": "npm run build",
    "dev": "npm run build && ts-node index.ts --src \"C:\\temp\\j2m_in.json\""
  },
  "author": "Mike Rogers",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.2",
    "nodemon": "^2.0.3",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/json-schema": "^7.0.5",
    "@types/jsonschema": "^1.1.1",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^15.0.5",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "codelyzer": "^6.0.0",
    "express": "^4.17.1",
    "json-schema": "^0.2.5",
    "json-schema-ref-parser": "^9.0.6",
    "jsonschema": "^1.2.6",
    "mocha": "^7.1.1",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.9.0",
    "winston": "^3.3.3",
    "yargs": "^15.4.1"
  }
}
